@model DirigoEdge.Areas.Admin.Models.ViewModels.Media

<tr class="new-row">
    <td class="thumbnail hidden-xs">
        <div class="image" data-thumb="@Model.Path"></div>
    </td>
    <td class="title">
        @Model.Path.Split('/').Last()
    </td>
    <td>
        @Model.CreateDate.ToString("MM/dd/yyyy")
    </td>
    <td class="hidden-xs copyImg">
        <input type="text" name="copy" class="copyText form-control" value="@Model.Path" />
        <button class="btn small btn-default copy">Copy</button>
    </td>
    <td>
        <a class="preview btn btn-info btn-sm" href="@Model.Path" target="_blank" data-src="@Model.Path">Preview</a>
        <button class="delete btn btn-danger btn-sm disabled" title="Refresh page to enable delete" data-toggle="tooltip" data-placement="top" data-src="@Model.Path">Delete</button>
    </td>
</tr>
